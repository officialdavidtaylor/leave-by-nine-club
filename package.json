{
  "name": "leave-by-nine-club",
  "version": "0.1.2",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "dev": "remix vite:dev",
    "dev:docker": "docker run -it -p 5173:5173 -v ./app:/app/app -v ./db:/app/db $(docker build -f Dockerfile.dev -q --no-cache .)",
    "build": "remix vite:build",
    "build:docker:arm64": "docker build -t officialdavidtaylor/leave-by-nine-club:$(yarn info leave-by-nine-club | grep -Eo '\\d\\.\\d\\.\\d' -m 1) --no-cache .",
    "build:docker:amd64": "docker build --platform linux/amd64 -t officialdavidtaylor/leave-by-nine-club:$(yarn info leave-by-nine-club | grep -Eo '\\d\\.\\d\\.\\d' -m 1)-amd64 --no-cache .",
    "start": "remix-serve ./build/server/index.js",
    "start:docker": "dotenv -e .env -- docker run -it -p 3000:3000 officialdavidtaylor/leave-by-nine-club",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:seed": "tsx ./db/seed.ts",
    "db:studio": "drizzle-kit studio --host 127.0.0.1 --port 3000 --verbose",
    "lint": "eslint --ignore-path .gitignore --cache --cache-location ./node_modules/.cache/eslint .",
    "test": "vitest",
    "typecheck": "tsc"
  },
  "dependencies": {
    "@libsql/client": "^0.14.0",
    "@remix-run/node": "^2.13.1",
    "@remix-run/react": "^2.13.1",
    "@remix-run/serve": "^2.13.1",
    "dotenv": "^16.4.5",
    "drizzle-orm": "^0.35.3",
    "isbot": "^4.1.0",
    "lucide-react": "^0.453.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "sqlite3": "^5.1.7",
    "uniqid": "^5.4.0"
  },
  "devDependencies": {
    "@faker-js/faker": "^9.0.3",
    "@remix-run/dev": "^2.13.1",
    "@types/node": "^22.7.7",
    "@types/react": "^18.2.20",
    "@types/react-dom": "^18.2.7",
    "@types/uniqid": "^5",
    "@typescript-eslint/eslint-plugin": "^6.7.4",
    "@typescript-eslint/parser": "^6.7.4",
    "autoprefixer": "^10.4.19",
    "dotenv-cli": "^7.4.2",
    "drizzle-kit": "^0.26.2",
    "eslint": "^8.38.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "postcss": "^8.4.38",
    "prettier": "3.3.3",
    "prettier-plugin-tailwindcss": "^0.6.8",
    "tailwindcss": "^3.4.4",
    "tsx": "^4.19.1",
    "typescript": "^5.1.6",
    "vite": "^5.1.0",
    "vite-tsconfig-paths": "^4.2.1",
    "vitest": "^2.1.3"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "packageManager": "yarn@4.5.0"
}
